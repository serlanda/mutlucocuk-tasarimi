#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/har-validator@2.0.6/node_modules/har-validator/bin/node_modules:/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/har-validator@2.0.6/node_modules/har-validator/node_modules:/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/har-validator@2.0.6/node_modules:/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/har-validator@2.0.6/node_modules/har-validator/bin/node_modules:/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/har-validator@2.0.6/node_modules/har-validator/node_modules:/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/har-validator@2.0.6/node_modules:/mnt/c/Users/mehmet/Desktop/salah-yudin/dunder-mifflin-x-salah-yudin/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../har-validator/bin/har-validator" "$@"
else
  exec node  "$basedir/../har-validator/bin/har-validator" "$@"
fi
